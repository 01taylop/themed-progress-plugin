{
  "name": "themed-progress-plugin",
  "description": "A Webpack plugin featuring an emoji-themed loading bar for a fun and aesthetic build process.",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/01taylop/themed-progress-plugin.git"
  },
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "main": "./lib/index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib",
    "index.d.ts"
  ],
  "scripts": {
    "build": "rimraf lib && yarn run build:cjs && yarn run build:esm",
    "build:cjs": "babel src -d lib --env-name cjs --out-file-extension .cjs",
    "build:esm": "babel src -d lib --env-name esm --out-file-extension .js",
    "prepublishOnly": "yarn test --coverage && yarn build && yarn test:bundles",
    "preview": "node ./scripts/preview",
    "test": "jest",
    "test:bundles": "jest --config ./jest/cjs.config.ts && jest --config ./jest/esm.config.ts"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@babel/cli": "7.28.3",
    "@babel/core": "7.28.3",
    "@babel/preset-env": "7.28.3",
    "@types/jest": "30.0.0",
    "babel-plugin-add-import-extension": "1.6.0",
    "jest": "30.1.3",
    "knip": "5.63.1",
    "rimraf": "5.0.10",
    "semver": "7.7.2",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "typescript": "5.9.2",
    "webpack": "5.101.3"
  },
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "keywords": [
    "emoji",
    "loading",
    "plugin",
    "progress",
    "themed",
    "webpack"
  ],
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
