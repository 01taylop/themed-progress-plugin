{
  "name": "themed-progress-plugin",
  "description": "A Webpack plugin featuring emoji-themed progress bars for a fun and engaging build process.",
  "version": "1.0.1",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/01taylop/themed-progress-plugin.git"
  },
  "type": "module",
  "engines": {
    "node": ">=18.18.0"
  },
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "exports": {
    ".": {
      "import": "./lib/index.js",
      "require": "./lib/index.cjs"
    }
  },
  "files": [
    "lib"
  ],
  "scripts": {
    "build": "rimraf lib && rollup -c",
    "prepublishOnly": "yarn test --coverage && yarn build && yarn test:integration",
    "preview": "tsx ./scripts/preview",
    "test": "jest src/",
    "test:integration": "jest tests/"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "chalk": "^4.1.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "16.0.1",
    "@rollup/plugin-terser": "0.4.4",
    "@rollup/plugin-typescript": "12.1.4",
    "@types/jest": "30.0.0",
    "jest": "30.1.3",
    "knip": "5.63.1",
    "rimraf": "5.0.10",
    "rollup": "4.50.1",
    "ts-jest": "29.4.1",
    "ts-node": "10.9.2",
    "tslib": "2.8.1",
    "tsx": "4.20.5",
    "typescript": "5.9.2",
    "webpack": "5.101.3"
  },
  "author": "Patrick Taylor <hello@patricktaylor.dev>",
  "keywords": [
    "emoji",
    "loading",
    "plugin",
    "progress",
    "themed",
    "webpack"
  ],
  "license": "MIT",
  "packageManager": "yarn@4.6.0+sha512.5383cc12567a95f1d668fbe762dfe0075c595b4bfff433be478dbbe24e05251a8e8c3eb992a986667c1d53b6c3a9c85b8398c35a960587fbd9fa3a0915406728"
}
